{
    "geode": "5.0.0-beta.2",
    "gd": {
        "win": "2.2081",
        "android": "2.2081",
        "mac": "2.2081",
        "ios": "2.2081"
    },
    "id": "cheeseworks.fastpopups",
    "name": "Fast Pop-ups",
    "version": "1.0.0-beta.2",
    "developers": [
        "Cheeseworks"
    ],
    "description": "Make all pop-ups use the 'Fast Menu' setting.",
    "links": {
        "source": "https://github.com/BlueWitherer/FastPopups",
        "homepage": "https://www.cheeseworks.gay/",
        "community": "https://www.dsc.gg/cheeseworks"
    },
    "tags": [
        "offline",
        "enhancement",
        "bugfix"
    ],
    "dependencies": {
        "alphalaneous.alphas_geode_utils": ">=1.2.0"
    },
    "settings": {
        "no-force": {
            "type": "bool",
            "name": "Don't Force Animation",
            "description": "Avoid forcing the elastic animation on pop-ups that fully intend to disable this behavior. <cr>Re-enabling it could result in unexpected behavior.</c>",
            "default": true
        }
    },
    "early-load": false
}